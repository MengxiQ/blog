<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.mybatis.dao.IsubfieldDao">
    <select id="findSubfieldByTitle" resultType="subfield" parameterType="String">
        select * from subfield where title = #{title}
    </select>
</mapper>